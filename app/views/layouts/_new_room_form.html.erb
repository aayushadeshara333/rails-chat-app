<%= form_with model: @room, remote: true, class: "d-flex justify-content-between w-100 mb-3 text-dark", data: { controller: "reset-form", action: "turbo:submit-end->reset-form#reset" } do |form| %>
  <%= form.text_field :name, class: "form-control", autocomplete: "off"%>
  <%= form.submit "Create", data: { disable_with: false }, class: "btn btn-primary mx-1"%>
<% end %>
